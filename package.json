{
  "name": "FeedaFile",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push",
    "serve": "node-dev server.js"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LucyMoore/FeedaFile"
  },
  "author": "LucyMoore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucyMoore/FeedaFile/issues"
  },
  "homepage": "https://github.com/LucyMoore/FeedaFile",
  "dependencies": {
    "express": "^4.13.4"
  },
  "devDependencies": {
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
